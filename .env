# Gemini API настройки
GEMINI_API_KEY=
GEMINI_MODEL=gemini-2.5-flash

# Telegram настройки
TELEGRAM_BOT_TOKEN=
BOT_NAME=GeminiTranslationBot
# Многопоточность
THREAD_POOL_CORE_SIZE=10
THREAD_POOL_MAX_SIZE=50
THREAD_POOL_QUEUE_SIZE=1000
THREAD_KEEP_ALIVE_TIME=60

# Rate limiting
RATE_LIMIT_PER_MINUTE=60
RATE_LIMIT_PER_HOUR=1000

# Кеширование
CACHE_MAX_SIZE=1000
CACHE_EXPIRE_MINUTES=60

# Очередь сообщений
MAX_RETRY_ATTEMPTS=3
RETRY_DELAY_MS=1000

# Статистика
ENABLE_STATS=true
STATS_LOG_FILE=bot_stats.log

# Всплывающие подсказки
ENABLE_INLINE_SUGGESTIONS=true